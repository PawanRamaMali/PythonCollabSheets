        "\n",
        "#Code/Answers below :\n",
        "def func2(word):\n",
        "  count_upper=count_lower=0\n",
        "  a='ABCDEFGHIJKLMNOPQRSTUVWXYZ' \n",
        "  b='abcdefghijklmnopqrstuvwxyz'\n",
        "  for i in word:\n",
        "    if i in a:\n",
        "        count_upper+=1\n",
        "    elif i in b:\n",
        "        count_lower+=1  \n",
        "    else:\n",
        "        continue  \n",
        "        i+=1\n",
        "     \n",
        "  print(f\"\\ncount_upper:{count_upper} \\ncount_lower: {count_lower}\")  \n",
        "string=input(\"Enter any string: \")  \n",
        "func2(string)"
