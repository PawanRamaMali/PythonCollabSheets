        "#Solved by : Taiba Naz\n",
        "\n",
        "#Code/Answers below :\n",
        "text=input('Enter a string: ')\n",
        "count=0\n",
        "print(\"consonants: \",end=\"\")\n",
        "for i in range(len(text)):\n",
        "  if text[i] in 'aeiou':\n",
        "      count+=1\n",
        "  else:\n",
        "    print(text[i],end=\",\") \n",
        "print()    \n",
        "print(f\"count of vowel: {count}\")\n"
      ],
