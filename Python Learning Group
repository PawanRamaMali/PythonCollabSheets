        "#Solved by : VINAYA\n",
        "\n",
        "#Code/Answers below :\n",
        "def case_change(word):\n",
        "    count = 0\n",
        "    result = ''\n",
        "    for l in word:\n",
        "        if count % 2 == 0:\n",
        "            if l.isupper():\n",
        "                result += l.lower()\n",
        "            else:\n",
        "                result += l.upper()\n",
        "        else:\n",
        "            result += l\n",
        "        count += 1\n",
        "    return result\n",
        "str=input('Enter word:')\n",
        "case_change(str)"
      ],
      "execution_count": null,
      "outputs": [
