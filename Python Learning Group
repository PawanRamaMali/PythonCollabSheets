        "\n",
        "vowels = consonants = digits = spaces = symbols = 0\n",
        "\n",
        "for i in inp:\n",
        "  if i.lower() in \"aeiou\":\n",
        "    vowels+=1\n",
        "  elif i.isalpha():\n",
        "    consonants+=1\n",
        "  elif i.isdigit():\n",
        "    digits+=1\n",
        "  elif i.isspace():\n",
        "    spaces+=1\n",
        "  else:\n",
        "    symbols+=1\n",
        "\n",
        "print(f\"\\nVowels : {vowels} \\nConsonants : {consonants } \\nDigits : {digits} \\nSpaces : {spaces} \\nSymbols:{symbols}\")"
