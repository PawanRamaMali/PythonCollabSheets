        "count_A=count_B = 0\n",
        "z=input(\"enter a string: \")\n",
        "for i in range(len(z)):\n",
        "  if z[i]=='A':\n",
        "    count_A+=1\n",
        "  elif z[i]==\"B\":\n",
        "    count_B+=1\n",
        "  else:\n",
        "    continue  \n",
        "print(count_A,count_B)\n",
        "if count_A >count_B:\n",
        "  print(\"String contains more As\")\n",
        "elif count_A <count_B:\n",
        "  print(\"String contains more Bs\")  \n",
