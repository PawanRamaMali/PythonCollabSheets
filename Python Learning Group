        "string=input(\"Enter a string: \")\n",
        "print(len(string))\n",
        "n=int((len(string))/2)#middle index\n",
        "\n",
        "FH=string[:(n)]  #First half of string\n",
        "SH=string[n:]    #Second half of string \n",
        "print(f\"First half of string: {FH}\")\n",
        "print(f\"Second half of string: {SH}\")"
