        "  count_upper=count_lower=0\n",
        "  import string\n",
        "  a=string.ascii_uppercase\n",
        "  b=string.ascii_lowercase\n",
        "  for i in word:\n",
        "    if i in a:\n",
        "        count_upper+=1\n",
        "    elif i in b:\n",
        "        count_lower+=1  \n",
        "    else:\n",
        "        continue  \n",
        "        i+=1\n",
        "     \n",
        "  print(f\"\\ncount_upper:{count_upper} \\ncount_lower: {count_lower}\")  \n",
