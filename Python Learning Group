        "#with loops\n",
        "str1=\"python\"\n",
        "str2=\"learning\"\n",
        "def str_evod(st1,st2):\n",
        "  st3=\"\"\n",
        "  for i in range(0,len(st1)):\n",
        "    if i%2==0:\n",
        "      st3+=st1[i]\n",
        "  for i in range(0,len(st2)):\n",
        "    if i%2!=0:\n",
        "      st3+=st2[i]\n",
        "  print(\"This output is with loops: \",st3)\n",
        "str_evod(str1,str2)\n",
        "##Without loops\n",
        "def str_evod_wl(st1,st2):\n",
        "  str3=st1[::2]+st2[1::2]\n",
