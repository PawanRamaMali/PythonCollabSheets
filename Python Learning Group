        "#Code/Answers below \n",
        "\n",
        "def Upper2(x):\n",
        "    x = x.lower()\n",
        "    y = x.split()\n",
        "    word = []\n",
        "    \n",
        "    for i in y:\n",
        "        k = i[1].upper()\n",
        "        z = \"\".join([i[0],k,i[2:]]) #z = \"\"+i[0]+i[1].upper()+i[2:]\n",
        "        word.append(z)\n",
        "   \n",
        "    w = \" \".join(word) \n",
        "    print(w)\n",
        "\n",
        "Upper2(\"THE Blue MOON\")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "tHe bLue mOon\n"
          ],
          "name": "stdout"
        }
      ]
    },
    {
      "cell_type": "markdown",
