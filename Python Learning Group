        "#Code/Answers below :\n",
        "import random\n",
        "import string\n",
        "\n",
        "def get_random_string(n):\n",
        "  letters=string.ascii_lowercase\n",
        "  result_str=''.join(random.choice(letters)for i in range(n))\n",
        "  print(result_str)\n",
        "get_random_string(10)    "
