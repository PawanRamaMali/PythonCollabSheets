        "\n",
        "def fun1(word):\n",
        "  count=0\n",
        "  result='' \n",
        "  for i in word:\n",
        "    if count%2==0 and i in 'aeiou':\n",
        "      result+='#'\n",
        "    elif count%2!=0 and i not in 'aeiou': \n",
        "      result+='$'\n",
        "    else:\n",
        "      result+=i  \n",
        "    \n",
        "    count+=1\n",
        "  return result\n",
        "\n",
        "string=input(\"Enter any string:\").lower()\n",
        "fun1(string)"
      ],
