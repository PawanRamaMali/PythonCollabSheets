    {
      "cell_type": "code",
      "metadata": {
        "id": "Ar4aP2yWNrrl",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 67
        },
        "outputId": "a4686b32-096e-4875-db89-d32430de0b7c"
