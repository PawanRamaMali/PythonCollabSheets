        "#Alternative Solution by : Saket (Intern)\n",
        "\n",
        "#Code Below :\n",
        "def roman(w):\n",
        "    r = w.upper()\n",
        "    check_variable = 0\n",
        "    for i in r:\n",
        "        if i not in \"IVX\":\n",
        "            check_variable = 1\n",
        "            break\n",
        "    if check_variable == 0:\n",
        "        if len(r)<8 and len(r)!=0:\n",
        "            if \"I\" in r[0]:\n",
        "                if r.count(\"I\")>3:\n",
        "                    print(\"\\nError! The given roman number is wrong.\")\n",
        "                elif (\"V\" in r and r!=\"IV\") or (\"X\" in r and r!=\"IX\"):\n",
        "                    print(\"\\nError! The given roman number is wrong!\")\n",
        "                else:\n",
        "                    print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "            elif \"V\" in r[0]:\n",
        "                if r.count(\"V\")>1 or r.count(\"I\")>3 or r.count(\"X\")>0:\n",
        "                    print(\"\\nError! The given roman number is wrong!!\")\n",
        "                else:\n",
        "                    print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "            elif \"X\" in r[0]:\n",
        "                if r!=\"XXXIX\" and r.count(\"X\")>3:\n",
        "                    print(\"\\nError! The roman number inputted is invalid.\")\n",
        "                elif \"XI\" in r[0:2]: \n",
        "                    if (r!=\"XIX\" and r!=\"XIV\"): \n",
        "                        if (r.count(\"V\")>1 or r.count(\"I\")>3 or r.count(\"X\")>2 or r.count(\"IX\")>0 or r.count(\"IV\")>0):\n",
        "                            print(\"\\nError! The roman number inputted is invalid!\")\n",
        "                        else:\n",
        "                            print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "                    else:\n",
        "                        print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "                elif \"XI\" in r and \"IX\" not in r[-2:]:\n",
        "                    if r.count(\"V\")>1 or r.count(\"I\")>3 or r[r.find(\"V\"):].count(\"X\")>0 or r[r.find(\"I\"):].count(\"X\")>0 or r[r.find(\"XI\")+2:].count(\"IV\")>0 or r[r.find(\"XI\")+2:].count(\"VI\")>0:\n",
        "                        print(\"\\nError! The roman number inputted is invalid!!\")\n",
        "                    else:\n",
        "                        print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "                elif \"V\" in r:\n",
        "                    if r[r.find(\"V\"):].count(\"X\")>0 or r.count(\"V\")>1 or r.count(\"I\")>3:\n",
        "                        print(\"\\nError! The roman number inputted is invalid!!!\")\n",
        "                    else:\n",
        "                        print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "                elif \"IX\" in r[-2:]:\n",
        "                    if r.count(\"V\")>0 or r.count(\"I\")>1 or r.count(\"XI\")>1:\n",
        "                        print(\"\\nError! The roman number inputted is invalid!!!!\")\n",
        "                    else:\n",
        "                        print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "                else:\n",
        "                    print(\"\\n\\tEntered roman number is valid and in range of 1 to 39!!\\n\") \n",
        "        else:\n",
        "            print(\"\\nError! The provided roman number is wrong or not in range of 1 to 39!\")\n",
        "    else:\n",
        "        print(\"\\nERROR! The roman number provided invalid or not in range of 1 to 39!!\")\n",
        "\n",
        "\n",
        "roman(input(\"\\nEnter roman number from 1 to 39 : \"))"
